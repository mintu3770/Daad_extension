{
  "manifest_version": 3,
  "name": "Mintu's - DAAD Helper",
  "version": "1.0",
  "description": "Custom tool to download DAAD shortlists.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "daad-helper@mintu.com",
      "strict_min_version": "109.0"
    }
  }
}
