{
  "manifest_version": 3,
  "name": "DAAD Helper",
  "version": "1.0",
  "description": "Automated tool to download DAAD shortlists to Excel.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "daad-helper@mintu.com",
      "strict_min_version": "109.0"
    },
    "gecko_android": {
      "strict_min_version": "113.0"
    }
  }
}
